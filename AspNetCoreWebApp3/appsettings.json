{
	"Logging": {
		"LogLevel": {
			"Default": "Information"
		}
	},
	"AllowedHosts": "*",
	"MassTransit": {
		"Connection": {
			"Hostname": "rabbitmq",
			"VirtualHost": "/",
			"Username": "rabbitmq",
			"Password": "rabbitmq"
		},
		"Retry": {
			"Count": "2",
			"IntervalInMs": "1000"
		},
		"Consumer": {
			"PrefetchCount": "16"
		}
	},
	"swagger": {
		"enabled": "true",
		"includeXmlComments": "true",
		"docName": "AspNetCoreWebApi3",
		"docVersion": "V1",
		"endpoint": "/swagger/v1/swagger.json",
		"endpointName": "AspNetCoreWebApi3",
		"routePrefix": ""
	}
}
