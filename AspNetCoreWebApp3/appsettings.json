{
	"Logging": {
		"LogLevel": {
			"Default": "Information"
		}
	},
	"AllowedHosts": "*",
	"rabbitmq": {
		"connection": {
			"hostname": "rabbitmq",
			"username": "rabbitmq",
			"password": "rabbitmq"
		},
		"exchange": {
			"name": "fhir_topic",
			"type": "topic",
			"durable": "false",
			"autoDelete": "true",
			"arguments": []
		},
		"queue": {
			"name": "",
			"exclusive": "1",
			"durable": "0",
			"autoDelete": "1",
			"arguments": []
		},
		"queueBinding": {
			"routingKeys": [ "#" ]
		}
	},
	"startup": {
		"swagger": {
			"enabled": "true",
			"includeXmlComments": "true",
			"docName": "AspNetCoreWebApi3",
			"docVersion": "V1",
			"swaggerEndpoint": "/swagger/v1/swagger.json",
			"swaggerEndpointName": "AspNetCoreWebApi3",
			"routePrefix": ""
		}
	}
}
